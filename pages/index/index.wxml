<view class='Jacky-index'>
  <view class='header'>
    <text>Jacky组件库</text>
  </view>
  <view class='container between'>
    <block wx:for="{{itemList}}" wx:key="{{item}}">
      <view class='item row' data-path="{{item.path}}" catchtap="toPath">
        <text>{{item.text}}</text>
      </view>
    </block>
  </view>
</view>